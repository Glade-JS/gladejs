{
    "name": "@gladejs/eleventy",
    "version": "0.5.0",
    "description": "The GladeJS Module adding support for Eleventy, move along ...",
    "keywords": [
        "gladejs",
        "eleventy",
        "rollup-plugin"
    ],
    "homepage": "https://gladejs.com",
    "repository": "github:gladejs/gladejs",
    "bugs": "https://github.com/gladejs/gladejs/issues",
    "author": {
        "name": "Aurélien Stébé",
        "email": "aurelien.stebe@gmail.com"
    },
    "type": "module",
    "main": "./dist/eleventy-rollup.js",
    "exports": "./dist/eleventy-rollup.js",
    "license": "MIT",
    "engines": {
        "npm": ">=6.14",
        "node": ">=14.0"
    },
    "os": [
        "darwin",
        "linux",
        "win32"
    ],
    "release": {
        "tagFormat": "eleventy-v${version}",
        "plugins": [
            [
                "@semantic-release/commit-analyzer",
                {
                    "releaseRules": [
                        {
                            "scope": "!@eleventy",
                            "release": false
                        }
                    ]
                }
            ],
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            [
                "@semantic-release/git",
                {
                    "message": "chore(release): @eleventy version ${nextRelease.version} :tada: "
                }
            ],
            "@semantic-release/github"
        ]
    },
    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "chokidar": "^3.5.1",
        "fs-extra": "^9.1.0",
        "glob": "^7.1.6",
        "normalize-path": "^3.0.0"
    },
    "peerDependencies": {
        "rollup": "^2"
    },
    "publishConfig": {
        "access": "public"
    }
}
